$PEDALY = BRZDU | PLYN | SPOJKU;
$CISLO = DESET | DVACET | TRICET | CTYRICET | PADESAT;
$OKOLIK = O (NULA | $CISLO) STUPNU [$CISLO VTERIN];
$SMERPRID = LEVY | PRAVY;
$SMEROKNO = LEVE | PRAVE;
$TYPSTER = PREDNI | ZADNI;
$TYPSVET = DALKOVA | POTKAVACI | MLHOVA;
$OBJEKT = $TYPSTER STERACE | $SMERPRID BLINKR | $TYPSVET SVETLA;
$CISLA = JEDNICKU | DVOJKU | TROJKU | CTYRKU | PETKU | SESTKU | ZPATECKU;
$SMER = (DOLEVA | DOPRAVA) $OKOLIK;


(SIL (NATOC VOLANT $SMER | SLAPNI NA $PEDALY | PRESTAN SLAPAT NA $PEDALY | ZAPNI $OBJEKT | VYPNI $OBJEKT | ZATRUB | ZARAD $CISLA | STAHNI $SMEROKNO $TYPSTER OKNO | ZVEDNI $SMEROKNO $TYPSTER OKNO) SIL)
